{
  "expo": {
    "name": "FillUpFinder",
    "slug": "FillUpFinder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.luischamorro.MapTest",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC1g8jOow_Tw-DZ2JQQu-1J7YiBh-Jnun4"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
